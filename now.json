{
    "version": 2,
    "builds": [
        {"src": "/server.js", "use": "@now/node"}

    ],
    "routes":[
        {
            "src":"/auth/(.*)",
            "dest":"/src/api/auth.js"
        },
        {
            "src":"/contacts/(.*)",
            "dest":"/src/api/contacts.js"
        },
        {
            "src":"/users/(.*)",
            "dest":"/src/api/users.js"
        }
    ]
}